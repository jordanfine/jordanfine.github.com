body, h1, h2, h3, h4, h5, h6, p, li, a
	margin: 5px 0
	font-family: 'Lato', sans-serif
	font-weight: $headings-font-weight
	line-height: $headings-line-height
	color: inherit
	text-rendering: optimizeLegibility
	small
	font-weight: normal
	line-height: 1

a
	color: $white
	text-decoration: none

p
	line-height: 1.2
	padding: 5px 0